{
  "version": 2,
  "alias": "brush.tips",
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": { "distDir": "build" }
    },
    {
      "src": "data/*.json",
      "use": "@now/static"
    },
    {
      "src": "**/*.css",
      "use": "@now/static"
    },
    {
      "src": "index.html",
      "use": "@now/static"
    }
  ]
}
